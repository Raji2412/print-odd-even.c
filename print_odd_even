#include <stdio.h>

int main() {
   int n;
   scanf("%d",&n);
   int arr[50];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   for(int i=0;i<n;i++)
   {
       if((arr[i] & 1)==0)
       {
           printf("e ");
           
       }
       else
       {
           printf("o ");
       }
   }
    return 0;
